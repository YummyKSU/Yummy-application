<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout7"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light"
    tools:context=".createacount" >





    <ImageView
        android:id="@+id/logo2"
        android:layout_width="169dp"
        android:layout_height="121dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:src="@drawable/icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/newemil"
        android:layout_width="364dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/name"
        android:ems="10"
        android:hint="     Email"
        android:inputType="textPersonName"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:textColor="@android:color/background_dark"
        android:textColorHint="@android:color/darker_gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo2" />

    <EditText
        android:id="@+id/newpass"
        android:layout_width="364dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/name"
        android:ems="10"
        android:hint="     Password"
        android:inputType="textVisiblePassword"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:password="true"
        android:textColor="@android:color/black"
        android:textColorHint="@android:color/darker_gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/newemil" />


    <EditText
        android:id="@+id/usernamec"
        android:layout_width="364dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/name"
        android:hint="    Username"
        android:inputType="textPersonName|text"
        android:paddingLeft="5dp"
        android:paddingTop="5dp"
        android:textColor="@android:color/black"
        android:textColorHint="@android:color/darker_gray"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/newpass" />

    <EditText
        android:id="@+id/number"
        android:layout_width="364dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/name"
        android:hint="    PhoneNumber"
        android:inputType="textPersonName|text"
        android:paddingLeft="5dp"
        android:paddingTop="5dp"
        android:textColor="@android:color/background_dark"
        android:textColorHint="@android:color/darker_gray"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/usernamec" />


    <Button

        android:id="@+id/sginup"
        android:layout_width="364dp"
        android:layout_height="45dp"
        android:background="@drawable/login"
        android:text="Sign UP"
        android:textIsSelectable="true"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/radioGroup"
        app:layout_constraintStart_toStartOf="@+id/radioGroup"
        tools:ignore="DuplicateIds,MissingConstraints"
        tools:layout_editor_absoluteY="419dp" />

    <ImageView
        android:id="@+id/bottomlogin"
        android:layout_width="372dp"
        android:layout_height="40dp"


        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="32dp"
        android:background="#f0fefdfd"
        android:fadeScrollbars="true"
        android:scrollbarAlwaysDrawHorizontalTrack="true"
        android:scrollbarAlwaysDrawVerticalTrack="true"
        android:scrollbars="horizontal|vertical"
        android:src="#f0fefdfd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/sginup"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/sginup"
        app:layout_constraintTop_toBottomOf="@+id/sginup"
        app:layout_constraintVertical_bias="0.75" />

    <ImageView
        android:id="@+id/qus1"
        android:layout_width="188dp"
        android:layout_height="19dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"


        android:background="@android:color/background_light"
        android:src="@drawable/loginque"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/bottomlogin"
        app:layout_constraintVertical_bias="0.139"
        tools:ignore="NotSibling" />

    <ImageView
        android:id="@+id/loginpage"
        android:layout_width="71dp"
        android:layout_height="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:src="@drawable/loginphoto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.129"
        app:layout_constraintStart_toEndOf="@+id/qus1"
        app:layout_constraintTop_toTopOf="@+id/bottomlogin"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="NotSibling" />

    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/sginup"
        app:layout_constraintEnd_toEndOf="@+id/number"
        app:layout_constraintStart_toStartOf="@+id/number"
        app:layout_constraintTop_toBottomOf="@+id/number"
        tools:ignore="MissingConstraints">

        <RadioButton
            android:id="@+id/radioButton2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:button="@android:drawable/radiobutton_off_background"
            android:onClick="onRadioButtonClicked"
            android:text="Food delivery"
            android:textColor="@android:color/background_dark"
            tools:ignore="HardcodedText,MissingConstraints"
            tools:layout_editor_absoluteX="206dp"
            tools:layout_editor_absoluteY="379dp" />

        <RadioButton
            android:id="@+id/radioButton"
            android:layout_width="190dp"
            android:layout_height="wrap_content"
            android:button="@android:drawable/radiobutton_off_background"
            android:onClick="onRadioButtonClicked"
            android:text="Customer"
            android:textColor="@android:color/background_dark"
            tools:ignore="HardcodedText,MissingConstraints"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="379dp" />
    </RadioGroup>




</androidx.constraintlayout.widget.ConstraintLayout>
